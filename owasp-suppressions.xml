<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- 
        OWASP Dependency Check Suppressions
        These suppressions are for known false positives or accepted risks
        Review and update regularly
    -->
    
    <!-- Spring Boot related suppressions -->
    <suppress>
        <notes>Spring Boot transitive dependencies - false positive for CVE in different artifact</notes>
        <cve>CVE-2023-20863</cve>
    </suppress>
    
    <!-- H2 Database - only used in development/testing -->
    <suppress>
        <notes>H2 Database vulnerabilities - only used in development and test environments</notes>
        <gav regex="true">^com\.h2database:h2:.*$</gav>
        <cve>CVE-2022-45868</cve>
    </suppress>
    
    <!-- Test dependencies -->
    <suppress>
        <notes>Test scope dependencies with lower risk tolerance</notes>
        <gav regex="true">^.*:.*:.*:test$</gav>
        <cvssBelow>7.0</cvssBelow>
    </suppress>
    
    <!-- Add more suppressions as needed based on your specific requirements -->
</suppressions>